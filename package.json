{
  "name": "mks-themekit-shopify",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "yarn watch & mkdir -p dist && theme watch -d dist --vars ${HOME}/Sites/themekit/.env",
    "deploy": "yarn build && theme deploy -d dist",
    "get-theme": "theme get -s=$(grep DEV_STORE .env | cut -d '=' -f2)",
    "watch": "webpack --watch --mode development",
    "clean": "rm -rf dist",
    "build": "yarn clean && webpack --mode production",
    "lint": "npx eslint . --ext .js,.jsx",
    "compare-up": "yarn build && mv assets tempassets && cp -r dist/assets assets",
    "compare-down": "rm -rf assets && mv tempassets assets"
  },
  "devDependencies": {
    "@babel/core": "7.8.7",
    "@babel/plugin-proposal-do-expressions": "7.8.3",
    "@babel/plugin-proposal-export-default-from": "7.8.3",
    "@babel/plugin-proposal-export-namespace-from": "7.8.3",
    "@babel/plugin-proposal-function-bind": "7.8.3",
    "@babel/plugin-proposal-function-sent": "7.8.3",
    "@babel/plugin-proposal-json-strings": "7.8.3",
    "@babel/plugin-proposal-logical-assignment-operators": "7.8.3",
    "@babel/plugin-proposal-nullish-coalescing-operator": "7.8.3",
    "@babel/plugin-proposal-numeric-separator": "7.8.3",
    "@babel/plugin-proposal-optional-chaining": "7.8.3",
    "@babel/plugin-proposal-pipeline-operator": "7.8.3",
    "@babel/plugin-proposal-throw-expressions": "7.8.3",
    "@babel/plugin-syntax-dynamic-import": "7.8.3",
    "@babel/plugin-syntax-import-meta": "7.8.3",
    "@babel/plugin-transform-runtime": "7.8.3",
    "@babel/polyfill": "7.8.7",
    "@babel/preset-env": "7.8.7",
    "@babel/preset-react": "7.8.3",
    "@babel/runtime-corejs2": "7.8.7",
    "babel-core": "6.26.3",
    "babel-eslint": "10.1.0",
    "babel-loader": "8.0.6",
    "babel-plugin-import": "1.13.0",
    "babel-plugin-transform-optional-chaining": "7.0.0-beta.3",
    "copy-webpack-plugin": "^5.1.2",
    "cssnano": "4.1.10",
    "eslint": "5.16.0",
    "eslint-config-prettier": "6.10.0",
    "eslint-import-resolver-alias": "1.1.2",
    "eslint-plugin-babel": "5.3.0",
    "eslint-plugin-filenames": "1.3.2",
    "eslint-plugin-fp": "2.3.0",
    "eslint-plugin-import": "2.20.1",
    "eslint-plugin-json": "2.1.0",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-prettier": "3.1.2",
    "eslint-plugin-react": "7.18.3",
    "eslint-plugin-react-hooks": "2.5.0",
    "html-webpack-plugin": "4.3.0",
    "mini-css-extract-plugin": "0.9.0",
    "postcss-loader": "3.0.0",
    "postcss-preset-env": "6.7.0",
    "prettier": "1.19.1",
    "react-dev-utils": "10.2.1",
    "sass-loader": "8.0.2",
    "style-loader": "1.1.3",
    "webpack": "4.42.0",
    "webpack-cli": "3.3.11"
  },
  "dependencies": {
    "debounce": "1.2.0",
    "jquery": "^3.5.1",
